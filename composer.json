{
    "name": "redeemly/catalogue",
    "description": "Laravel package for Redeemly Catalogue services integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Redeemly",
            "email": "support@redeemly.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Redeemly\\CatalogueIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Redeemly\\CatalogueIntegration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redeemly\\CatalogueIntegration\\Providers\\CatalogueIntegrationServiceProvider"
            ],
            "aliases": {
                "Catalogue": "Redeemly\\CatalogueIntegration\\Facades\\Catalogue"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
